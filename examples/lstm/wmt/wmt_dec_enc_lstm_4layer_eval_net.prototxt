name: "CaffeNet"

# input data
layers {
  name: "data"
  type: HDF5_DATA
  top: "encoder_data"
  top: "decoder_data"
  top: "stage_indicators"
  top: "targets"
  top: "encoder_cont"
  top: "decoder_cont"
  top: "encoder_to_decoder"
  hdf5_data_param {
    source: "./wmt_hdf5/buffer_100/train_batches/hdf5_chunk_list.txt"
    batch_size: 800
  }
  include: { phase: TRAIN }
}
layers {
  name: "data"
  type: HDF5_DATA
  top: "encoder_data"
  top: "decoder_data"
  top: "stage_indicators"
  top: "targets"
  top: "encoder_cont"
  top: "decoder_cont"
  top: "encoder_to_decoder"
  hdf5_data_param {
    source: "./wmt_hdf5/buffer_100/valid_batches/hdf5_chunk_list.txt"
    batch_size: 400
  }
  include: { phase: TEST stage: "test-on-test" }
}
layers {
  name: "data"
  type: HDF5_DATA
  top: "encoder_data"
  top: "decoder_data"
  top: "stage_indicators"
  top: "targets"
  top: "encoder_cont"
  top: "decoder_cont"
  top: "encoder_to_decoder"
  hdf5_data_param {
    source: "./wmt_hdf5/buffer_100/train_batches/hdf5_chunk_list.txt"
    batch_size: 400
  }
  include: { phase: TEST stage: "test-on-train" }
}

