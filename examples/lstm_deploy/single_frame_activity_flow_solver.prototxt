net: "hyb2_RGB_train_test.prototxt"
test_iter: 75 
test_state: { stage: 'test-on-test' }
test_interval: 100
#test_net_reset: false
base_lr: 0.001
lr_policy: "step"
gamma: 0.1
stepsize: 3000
display: 20
max_iter: 20000
momentum: 0.9
weight_decay: 0.005  #turn up if overfitting
snapshot: 5000
snapshot_prefix: "single_frame_all_layers_hyb"
solver_mode: GPU
device_id: 1 
#debug_info: false
random_seed: 1701
